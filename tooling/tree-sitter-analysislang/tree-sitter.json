{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "analysislang",
      "camelcase": "analysislang",
      "title": "AnalysisLang",
      "scope": "source.analysislang",
      "file-types": [
        "sem"
      ],
      "injection-regex": "^analysislang$",
      "class-name": "TreeSitterAnalysislang",
      "highlights": "queries/highlights.scm"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "Analysislang grammar for tree-sitter",
    "authors": [
      {
        "name": "Bram Vandenbogaerde",
        "email": "bram.vandenbogaerde@vub.be",
        "url": "https://vdb.space/"
      }
    ],
    "links": {
      "repository": "https://github.com/tree-sitter/tree-sitter-analysislang"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}